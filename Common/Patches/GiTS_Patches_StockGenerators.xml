<?xml version="1.0" encoding="utf-8" ?>

<!-- GiTS TraderKindDef Patches-->
<Patch>
<!-- *** Cyberbrain Base Patches ***
adds gitsBodyPartCyberbrainBase to all TraderKindDefs that offer Implants-->
  <Operation Class="PatchOperationInsert">
    <xpath>Defs/TraderKindDef/stockGenerators/li[@Class="StockGenerator_Tag" and tradeTag="TechHediff"]</xpath>
    <value>
      <li Class="StockGenerator_Tag">
        <tradeTag>GiTS_TechHediff_Bionics</tradeTag>
        <thingDefCountRange>1~3</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>
    </value>
  </Operation>
</Patch>